<section id='comicSection'>
  <div class="sec-tag comic-tag">
    <h3>Comics - Graphic Novels</h3>
  </div>
  
  <div class="sec-body comic-body">
    <a onclick="ListBook(event,'comic')" style="cursor: pointer">BROWSE MORE</a>
    <div class="book-stand">
      <% comicdata.Items.forEach(function(comic, index){ 
        var imgurl = "https://donkie-booket.s3-us-west-2.amazonaws.com/" + (comic.title.S).replace(" ","+") + "/cover.jpg";
        %>
        <div onclick="OpenBook(event, '<%= comic.title.S%>', 'comic')">
          <img src="<%= imgurl%>" >
          <p><%= comic.title.S%></p>
        </div>
      <%})%>
    </div>
    
  </div>
</section>

