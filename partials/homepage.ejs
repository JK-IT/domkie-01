<section id='mangaSection'>
  <div class="sec-tag manga-tag">
    <h3>Manga - Manhwa - Manhua</h3>
  </div>

  <div class="sec-body manga-body">
    <a onclick="ListBook(event, 'manga')" style="cursor: pointer">BROWSE MORE</a>
    <div class="book-stand">
      <% mangadata.Items.forEach(function(manga, index){ 
          var imgurl = "https://donkie-booket.s3-us-west-2.amazonaws.com/" + (manga.title.S).replace(" ","+") + "/cover.jpg";
        %>
        <div onclick="OpenBook(event, '<%= manga.title.S%>', 'manga')">
          <img src="<%= imgurl%>">
          <p><%= manga.title.S%></p>
        </div>
        
      <%})%>
    </div>
  </div>
</section>
<br>
<hr>
<br>

