
<section class="book-display-section">
  <div class="sub-navigate">
    <div class="sub-link">
      <ul>
        <li onclick="BookGenreLink(event, 'browse manga', 'mangaListing')" class="sub-link-active"><a >Browse All</a></li>
        <li onclick="BookGenreLink(event, 'manga', 'mangaListing')"><a>Manga</a></li>
        <li onclick="BookGenreLink(event, 'manhwa', 'mangaListing')"><a>Manhwa</a></li>
        <li onclick="BookGenreLink(event, 'manhua', 'mangaListing')"><a>Manhua</a></li>
      </ul>
    </div>
    <%- include('./search-form.ejs')%>
  </div>
  <div class="search-result">

  </div>
  <hr style="width: 50%">

  <div id="mangaListing" class="book-listing-stand">
    <% bookdata.Items.forEach(function(book, index){ 
      var imgurl = 'https://donkie-booket.s3-us-west-2.amazonaws.com/' + ((book.title.S).replace(" ","+")) + '/cover.jpg'; %>
    <div class="book-display" onclick="OpenBook(event, '<%= book.title.S%>', 'manga')">
      <img src="<%= imgurl%>">
      <p><%= book.title.S %></p>
    </div>
    <%})%>
    <%if (typeof bookdata.LastEvaluatedKey != 'undefined'){%>
    <button type="button" onclick="FetchMore(event, 'manga', 'mangaListing')" class="load-button" data-lastkey='<%= JSON.stringify(bookdata.LastEvaluatedKey)%>'>Load More</button>
    <%}else {%>
    <button type="button" data-lastkey="" disabled>Load More</button>
    <%}%>
  </div>
</section>